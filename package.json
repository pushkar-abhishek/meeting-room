{
  "name": "MeetingRoom",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc -p .",
    "prestart": "copyfiles views/**/*.pug dist/",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch": "nodemon --watch 'src/**/*.ts' --exec ts-node server.ts",
    "prod": "nodemon dist/server.js",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "tslint": "tslint --project tsconfig.json",
    "format": "prettier --write \"{,!(node_modules|dist|build|deploy|views|swagger|coverage)/**/}*.{ts,json}\"",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint:fix && npm run format && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts*": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "10.14.2"
  },
  "author": "Neosoft Tech",
  "license": "ISC",
  "dependencies": {
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.7.1",
    "@types/randomstring": "^1.1.6",
    "@types/speakeasy": "^2.0.5",
    "bcrypt": "3.0.6",
    "body-parser": "^1.18.3",
    "change-case": "^3.1.0",
    "cors": "2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-validator": "5.3.1",
    "http-status-codes": "1.3.2",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.14",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.26",
    "mongoose": "^5.6.4",
    "mongoose-paginate": "5.0.3",
    "nexmo": "^2.4.1",
    "nodemailer": "6.2.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "pug": "2.0.4",
    "randomstring": "^1.1.5",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.0.7",
    "winston": "^3.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "4.14.134",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.7",
    "@types/mongoose-paginate": "5.0.6",
    "@types/nodemailer": "6.2.0",
    "@types/pug": "2.0.4",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.2"
  }
}